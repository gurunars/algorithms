{
    "python.languageServer": "Pylance",
    "python.analysis.typeCheckingMode": "strict",
    "python.analysis.completeFunctionParens": true,
    "python.pythonPath": ".venv/bin/python",
    "python.formatting.blackPath": ".venv/bin/black",
    "python.formatting.provider": "black",
    "editor.formatOnSave": true,
    "python.testing.pytestPath": ".venv/bin/pytest",
    "python.testing.pytestEnabled": true,
    "files.associations": {
        "*.tcc": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "*.cph": "cpp",
        "tuple": "cpp"
    },
    "python.testing.pytestArgs": [
        "cses"
    ],
    "python.testing.unittestEnabled": false,
    "restructuredtext.syntaxHighlighting.disabled": true,
    "esbonio.sphinx.confDir": "${workspaceFolder}/algorithms",
    "esbonio.sphinx.buildDir": "${workspaceFolder}/build",
    "esbonio.sphinx.srcDir": "${workspaceFolder}/algorithms",
    "restructuredtext.linter.doc8.executablePath": "${workspaceFolder}/.venv/bin/dock8",
    "restructuredtext.linter.rstcheck.executablePath": "${workspaceFolder}/.venv/bin/rstcheck",
    "restructuredtext.linter.rst-lint.executablePath": "${workspaceFolder}/.venv/bin/rst-lint",
    "restructuredtext.linter.run": "onType",
    "files.trimTrailingWhitespace": true,
    "editor.renderWhitespace": "all"
}